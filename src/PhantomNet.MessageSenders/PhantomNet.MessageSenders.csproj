<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>PhantomNet.MessageSenders class library</Description>
    <Copyright>Green Grass</Copyright>
    <AssemblyTitle>PhantomNet.MessageSenders</AssemblyTitle>
    <VersionPrefix>1.0.3-beta</VersionPrefix>
    <Authors>Minh Nguyen</Authors>
    <TargetFramework>netstandard1.6</TargetFramework>
    <AssemblyName>PhantomNet.MessageSenders</AssemblyName>
    <PackageId>PhantomNet.MessageSenders</PackageId>
    <PackageProjectUrl>https://github.com/green-grass/PhantomNet-Core</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/green-grass/PhantomNet-Core/blob/master/LICENSE</PackageLicenseUrl>
    <NetStandardImplicitPackageVersion>1.6.1</NetStandardImplicitPackageVersion>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MailKit" Version="1.12.0" />
    <PackageReference Include="Microsoft.Extensions.Options" Version="1.1.1" />
  </ItemGroup>

</Project>
